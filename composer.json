{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "coderstephen/slack-client": "^0.3.0",
    "symfony/yaml": "^4.1",
    "doctrine/orm": "^2.6",
    "psr/container": "^1.0",
    "pimple/pimple": "~3.0",
    "vlucas/phpdotenv": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "bootstrap/helpers.php"
    ],
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  }
}
